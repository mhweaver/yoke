{
	"prefix": "test-",
	"maxthreads": 5,
	"defaultProfile": {
		"name":"default",
		"noconcurrent": false,
		"before": [],
		"requiredFiles" : [
			"input", 
			"input.expected",
			"output.expected",
			"error.expected"
		],
		"createRequired":true,
		"command":"xargs echo",
		"stdin":["input"],
		"stdout":"output",
		"stderr":"error",
		"limitOutput": 20,
		"maxTimePerCommand": 30,
		"pass" : {
			"match": [
				["input.expected", "input"],
				["error.expected", "error"]
			],
			"rmatch": [
				["output.expected", "output"]
			],
			"zeroExit": true,
			"limitReached": false
		},
		"after":[],
		"next": null
	}
}